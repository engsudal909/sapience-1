{
  "name": "@sapience/relayer",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "tsx src/server.ts",
    "lint": "eslint src --quiet",
    "lint:fix": "eslint src --fix --quiet && pnpm format",
    "format": "prettier --write src",
    "bot": "tsx src/botExample.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:proxy": "node test-proxy.js",
    "test:proxy:direct": "node test-proxy.js --direct",
    "load-test": "tsx load-test.ts",
    "load-test:help": "tsx load-test.ts --help"
  },
  "dependencies": {
    "@sapience/sdk": "workspace:*",
    "@sentry/node": "8.49.0",
    "dotenv": "16.4.5",
    "envalid": "8.1.1",
    "prom-client": "15.1.3",
    "viem": "2.21.49",
    "ws": "8.18.3"
  },
  "devDependencies": {
    "@eslint/js": "9.18.0",
    "@types/node": "20.17.10",
    "@types/ws": "8.18.1",
    "@vitest/coverage-v8": "2.1.8",
    "eslint": "9.15.0",
    "globals": "15.11.0",
    "prettier": "3.3.3",
    "tsx": "4.19.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.21.0",
    "vitest": "2.1.8"
  },
  "engines": {
    "node": ">=24.11.0 <25.0.0",
    "pnpm": "9.x"
  }
}

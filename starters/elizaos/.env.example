# Solana Configuration
SOLANA_PUBLIC_KEY=
SOLANA_PRIVATE_KEY=

### SAPIENCE AUTONOMOUS MODE ###
# Controls which autonomous cycles run on startup and at intervals
# Options: forecast, trade, or both (comma-separated)
# Examples:
#   AUTONOMOUS_MODE=forecast,tradeforecast           # Run forecast cycle only
#   AUTONOMOUS_MODE=forecast,tradetrade              # Run trading cycle only  
#   AUTONOMOUS_MODE=forecast,tradeforecast,trade     # Run both forecast and trade cycles
AUTONOMOUS_MODE=forecast,trade

# Autonomous cycle interval (milliseconds)
FORECAST_INTERVAL_MS=14400000          # 4 hours between forecast cycles             # 5 minutes between cycles

### SAPIENCE FORECASTING CONFIGURATION ###
# Settings for autonomous forecasting/attestation
MIN_FORECAST_CONFIDENCE=0.85           # Conservative: 85% minimum for forecasts             # Minimum confidence for attestations (0-1)
FORECAST_BATCH_SIZE=10                   # Number of markets to process per cycle
PROBABILITY_CHANGE_THRESHOLD=10            # Percentage change required for re-attestation
MIN_HOURS_BETWEEN_ATTESTATIONS=24          # Hours between attestations on same market

### SAPIENCE TRADING CONFIGURATION ###
# This implementation uses the current Foil contract interface with struct parameters.
# It's compatible with both Arbitrum and Base networks via configurable CHAIN_ID.
# See: https://docs.sapience.xyz/builder-guide/guides/forecasting-agent

# Enable/disable trading (also enabled automatically when AUTONOMOUS_MODE includes 'trade')
ENABLE_TRADING=false

# Trading wager amounts
TRADING_WAGER_AMOUNT=250000000000000000   # TRADING_WAGER_AMOUNT=1000000000000000000.25 in USDe (18 decimals)   # $1 in USDe (18 decimals: 1 * 10^18)

# Trading service configuration
MIN_TRADING_CONFIDENCE=0.15                # Aggressive: 15% minimum for trades                 # Minimum confidence for trading predictions (0-1)
TRADING_INTERVAL_MS=10800000              # 3 hours between trading cycles                # Hours between trade attempts
MIN_TRADING_PREDICTION_CHANGE=10           # Percentage change required for new trade
TRADING_MARKETS_FETCH_LIMIT=200            # Number of markets to fetch
TRADING_MARKETS_SKIP=0                     # Number of markets to skip

# WebSocket and auction settings
TRADING_AUCTION_TIMEOUT_MS=300000          # 5 minutes auction timeout
TRADING_KEEPALIVE_INTERVAL_MS=20000        # 20 seconds keepalive ping
TRADING_STATUS_INTERVAL_MS=30000           # 30 seconds status update interval

# Contract addresses (Arbitrum)
UMA_RESOLVER_ADDRESS=0x2cc1311871b9fc7bfcb809c75da4ba25732eafb9
PREDICTION_MARKET_ADDRESS=0xb04841cad1147675505816e2ec5c915430857b40
USDE_TOKEN_ADDRESS=0xfEb8C4d5eFbaFf6e928eA090Bc660c363f883DBA

# Token approval settings
USDE_APPROVAL_AMOUNT=1000000000000000000000000  # 1M USDe approval

# API endpoints
SAPIENCE_WS_URL=wss://api.sapience.xyz/auction
SAPIENCE_GRAPHQL_URL=https://api.sapience.xyz/graphql


# Market filtering
MAX_MARKET_HOURS=336                       # Only trade on markets ending within 14 days

# Market Maker (runs as separate process - see starters/market-maker)
MARKET_MAKER_ENABLED=false
